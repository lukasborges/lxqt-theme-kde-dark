Notification {
    color: #fff;
    border: 1px solid #000;
    background:#000;
    margin: 0px;
    border-radius: 5px;
}

QLabel {
    color: #fff;
}

#closeButton {
    margin: 3px;
    border-radius: 4px;
    border: 1px solid transparent;
    padding: 4px;
    qproperty-icon: url(lxqt-notificationd/window-close.svg);
}

#closeButton:hover {
    color: rgba(54, 54, 54, 100%);
    background: rgba(30, 145, 255, 30%);
    border: 1px solid rgba(30, 145, 255, 100%);
}

#appLabel {
    font-weight: bold;
}
